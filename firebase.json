{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      ".firebaserc",
      ".git",
      ".gitignore",
      "README.md",
      "pops_bingo_brief/*.md",
      "*.zip",
      "*.sh"
    ],
    "headers": [
      {
        "source": "audio/**",
        "headers": [{"key": "Cache-Control", "value": "no-cache, must-revalidate"}]
      },
      {
        "source": "emulator/**",
        "headers": [{"key": "Cache-Control", "value": "public, max-age=604800"}]
      }
    ]
  }
}

